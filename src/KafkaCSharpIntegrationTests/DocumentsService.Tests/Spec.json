{
  "asyncapi": "2.3.0",
  "info": {
    "title": "Example application",
    "version": "0.1.0",
    "license": {
      "name": "apache"
    }
  },
  "channels": {
    "user/signedup": {
      "subscribe": {
        "message": {
          "description": "A message describing that a user just signed up.",
          "payload": {
            "title": "MessageBody",
            "properties": {
              "specversion": {
                "type": "string",
                "examples": ["0.2"]
              },
              "schemaversion":  {
                "type": "string"
              },
              "type":  {
                "type": "string",
                "examples": [ "event-type" ]
              },
              "source":  {
                "type": "string",
                "examples": [ "http://localhost" ]
              },
              "subject":  {
                "type": "string",
                "examples": [ "event-subject" ]
              },
              "parent_trace":  {
                "type": "string",
              },
              "id":  {
                "type": "string",
                "examples": [ "event-id" ]
              },
              "time":  {
                "type": "string",
                "examples": [ "event-date-time" ]
              },
              "datacontenttype": {
                "type": "string",
                "examples": [ "application/json" ]
              },
              "data": {
                "type": "object",
                "title": "MessageData",
                "properties": {
                  "fullName": {
                    "type": "string",
                    "examples": [ "event-data-name"]
                  },
                  "email": {
                    "type": "string",
                    "examples": [ "event-data-email"]
                  },
                  "age": {
                    "type": "string",
                    "examples": [ "20" ]
                  }
                }
              }
            }
          }
        }
      }
    }
  }
}
